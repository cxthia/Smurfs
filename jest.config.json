{
  "preset": "react-native",
  "transform": {
    "^.+\\.tsx?$": "ts-jest"
  },
  "testMatch": ["**/__tests__/**/*.test.ts?(x)", "**/?(*.)+(spec|test).ts?(x)"],
  "moduleFileExtensions": ["ts", "tsx", "js", "jsx", "json", "node"],
  "setupFilesAfterEnv": ["@testing-library/jest-native/extend-expect"]
}
